@var:invT = 1d0/Tgas
@var:invsqrT = 1d0/sqrt(Tgas)
@var:Hnuclei = get_Hnuclei(n(:))
@var:kl13 = 1.12d-10*exp(-7.035d4*invT)
@var:kh13 = 6.5d-7*invsqrT*exp(-5.2d4*invT)*(1.d0-exp(-6d3*invT))
