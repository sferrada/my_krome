#cooling function for several species (especially metals)

#define here shortcut variables employed in excitation rates
@var:T2 = Tgas*1d-2
@var:invTgas = 1d0/Tgas
@var:lnT = log(Tgas)

@TgasUpperLimit:1d4

##############
#CARBON
metal:C
#level n: energy (K), degeneracy g
level 0: 0.e0, 1
level 1: 24.e0, 3
level 2: 63.e0, 5

#Aij (1/s)
1 -> 0, 7.9d-8
2 -> 1, 2.7d-7
2 -> 0, 2.1d-14

#collider, level_up, level_down, rate
H, 1, 0, 1.6d-10*(T2)**(.14)
H, 2, 0, 9.2d-11*(T2)**(.26)
H, 2, 1, 2.9d-10*(T2)**(.26)

H+, 1, 0, (9.6D-11 -1.8D-14*Tgas +1.9D-18*Tgas**2) *Tgas**(.45)
       if(Tgas > 5d3): 8.9D-10*Tgas**(.117)
H+, 2, 0, (3.1D-12 -6.D-16*Tgas +3.9d-20*Tgas**2) *Tgas
       if(Tgas > 5d3): 2.3D-9*Tgas**(.0965)
H+, 2, 1, (1.D-10 -2.2D-14*Tgas +1.7D-18*Tgas**2) *Tgas**(.70)
       if(Tgas > 5d3): 9.2D-9*Tgas**(.0535)

e, 1, 0, 2.88D-6*Tgas**(-.5)*EXP(-9.25141 -7.73782D-1*lnT +3.61184D-1*lnT**2 -1.50892D-2*lnT**3 -6.56325D-4*lnT**4)
	if(Tgas > 1D3): 2.88D-6*Tgas**(-.5) *EXP(-4.446D2 -2.27913D2*lnT +4.2595D1*lnT**2 -3.4762*lnT**3 +1.0508D-1*lnT**4)
e, 2, 0, 1.73D-6*Tgas**(-.5)*EXP(-7.69735 -1.30743*lnT +.697638*lnT**2 -.111338*lnT**3 +.705277D-2*lnT**4)
	if(Tgas > 1D3): 1.73D-6*Tgas**(-.5)*EXP(3.50609D2 -1.87474D2*lnT +3.61803D1*lnT**2 -3.03283*lnT**3 +9.38138D-2*lnT**4)
e, 2, 1, 1.73D-6*Tgas**(-.5)*EXP(-7.4387 -.57443*lnT +.358264*lnT**2 -4.18166D-2*lnT**3 +2.35272D-3*lnT**4)
	if(Tgas > 1D3): 1.73D-6*Tgas**(-.5)*EXP(3.86186D2 -2.02192D2*lnT +3.85049D1*lnT**2 -3.19268*lnT**3 +9.78573D-2*lnT**4)

#add para/ortho ratio 
para/ortho: 1/3
H2or, 1, 0, 8.7d-11 -6.6d-11*exp(-Tgas/218.3) + 6.6d-11*exp(-2.*Tgas/218.3)
H2or, 2, 0, 1.2d-10 -6.1d-11*exp(-Tgas/387.3)
H2or, 2, 1, 2.9d-10 -1.9d-10*exp(-Tgas/348.9)

H2pa, 1, 0, 7.9D-11 -8.7D-11*EXP(-Tgas/126.4) + 1.3D-10*EXP(-2.*Tgas/126.4)
H2pa, 2, 0, 1.1D-10 -8.6D-11*EXP(-Tgas/223.) + 8.7D-11*EXP(-2.*Tgas/223.)
H2pa, 2, 1, 2.7D-10 -2.6D-10*EXP(-Tgas/250.7) + 1.8D-10*EXP(-2.*Tgas/250.7)


endmetal

